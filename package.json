{
	"name": "typescript-server",
	"scripts": {
		"prebuild": "rimraf ./dist",
		"ts:build": "tsc",
		"start:dev": "ts-node-dev --respawn --transpile-only src/server.ts",
		"build": "npm run ts:build",
		"start": "node dist/server.js",
		"tslint": "tslint -c tslint.json -p tsconfig.json"
	},
	"keywords": [
		"node",
		"express",
		"typescript"
	],
	"author": "Cornelius Okeke",
	"license": "MIT",
	"dependencies": {
		"bcryptjs": "^3.0.2",
		"cors": "^2.8.5",
		"express": "^5.1.0",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^8.15.1"
	},
	"devDependencies": {
		"@types/bcryptjs": "^3.0.0",
		"@types/cors": "^2.8.18",
		"@types/express": "^5.0.2",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/mongoose": "^5.11.97",
		"@types/node": "^22.15.29",
		"rimraf": "^6.0.1",
		"ts-node-dev": "^2.0.0",
		"tslint": "^6.1.3",
		"typescript": "^5.8.3",
		"webpack": "^5.99.9"
	}
}
